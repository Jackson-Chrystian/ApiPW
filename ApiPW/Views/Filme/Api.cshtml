@section Scripts{
    @Scripts.Render("~/bundles/knockout")
    @Scripts.Render("~/bundles/filme")
}

<table>
    <thead>
        <tr>
            <th>Filme </th>
            <th>&nbsp Titulo</th>
            <th>Ano Lancamento</th>
            <th>&nbsp Genero</th>
            <th>&nbsp Produtora</th>
        </tr>

    </thead>

    <tbody data-bind="foreach: filmes">
        <tr>
            <td data-bind="text: IdFilme"></td>
            <td data-bind="text: Titulo"></td>
            <td data-bind="text: AnoLancamento"></td>
            <td data-bind="text: Genero"></td>
            <td>&nbsp</td>
            <td data-bind="text: Produtora"></td>
            <td>
                <button>Editar</button>
                <button>Excluir</button>
            </td> 

        </tr>

    </tbody>

</table>